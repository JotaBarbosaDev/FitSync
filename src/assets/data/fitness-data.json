{
  "exercises": [
    {
      "id": "1",
      "name": "Supino Reto",
      "muscleGroup": "Peito",
      "equipment": "Barra",
      "difficulty": "Intermediário",
      "instructions": [
        "Deite-se no banco com os pés apoiados no chão",
        "Segure a barra com pegada mais larga que os ombros",
        "Baixe a barra até o peito de forma controlada",
        "Empurre a barra de volta à posição inicial"
      ],
      "primaryMuscles": ["Peitoral Maior"],
      "secondaryMuscles": ["Tríceps", "Deltoides Anterior"],
      "tips": [
        "Mantenha os ombros pressionados contra o banco",
        "Não deixe a barra quicar no peito",
        "Mantenha os pulsos retos"
      ]
    },
    {
      "id": "2",
      "name": "Agachamento",
      "muscleGroup": "Pernas",
      "equipment": "Barra",
      "difficulty": "Intermediário",
      "instructions": [
        "Posicione a barra nos ombros",
        "Mantenha os pés na largura dos ombros",
        "Desça como se fosse sentar em uma cadeira",
        "Volte à posição inicial empurrando pelos calcanhares"
      ],
      "primaryMuscles": ["Quadríceps", "Glúteos"],
      "secondaryMuscles": ["Isquiotibiais", "Panturrilhas"],
      "tips": [
        "Mantenha o peito erguido",
        "Joelhos alinhados com os pés",
        "Desça até as coxas ficarem paralelas ao chão"
      ]
    },
    {
      "id": "3",
      "name": "Puxada Frontal",
      "muscleGroup": "Costas",
      "equipment": "Máquina",
      "difficulty": "Iniciante",
      "instructions": [
        "Sente-se na máquina e ajuste a almofada das coxas",
        "Segure a barra com pegada larga",
        "Puxe a barra em direção ao peito",
        "Retorne controladamente à posição inicial"
      ],
      "primaryMuscles": ["Latíssimo do Dorso"],
      "secondaryMuscles": ["Bíceps", "Romboides"],
      "tips": [
        "Mantenha o peito erguido",
        "Puxe com os músculos das costas, não com os braços",
        "Evite balançar o corpo"
      ]
    },
    {
      "id": "4",
      "name": "Rosca Direta",
      "muscleGroup": "Braços",
      "equipment": "Halter",
      "difficulty": "Iniciante",
      "instructions": [
        "Fique em pé com os halteres nas mãos",
        "Mantenha os cotovelos fixos ao lado do corpo",
        "Flexione os braços levantando os halteres",
        "Baixe controladamente à posição inicial"
      ],
      "primaryMuscles": ["Bíceps"],
      "secondaryMuscles": ["Antebraços"],
      "tips": [
        "Não balance o corpo",
        "Controle a descida",
        "Mantenha os pulsos firmes"
      ]
    },
    {
      "id": "5",
      "name": "Desenvolvimento com Halteres",
      "muscleGroup": "Ombros",
      "equipment": "Halter",
      "difficulty": "Intermediário",
      "instructions": [
        "Sente-se ou fique em pé com halteres nos ombros",
        "Empurre os halteres acima da cabeça",
        "Mantenha o core contraído",
        "Baixe controladamente aos ombros"
      ],
      "primaryMuscles": ["Deltoides"],
      "secondaryMuscles": ["Tríceps", "Trapézio"],
      "tips": [
        "Não arquee as costas excessivamente",
        "Mantenha os halteres alinhados",
        "Controle todo o movimento"
      ]
    },
    {
      "id": "6",
      "name": "Prancha",
      "muscleGroup": "Abdômen",
      "equipment": "Corpo Livre",
      "difficulty": "Iniciante",
      "instructions": [
        "Fique em posição de flexão apoiado nos antebraços",
        "Mantenha o corpo em linha reta",
        "Contraia o abdômen e glúteos",
        "Mantenha a posição pelo tempo determinado"
      ],
      "primaryMuscles": ["Core", "Abdômen"],
      "secondaryMuscles": ["Ombros", "Glúteos"],
      "tips": [
        "Não deixe o quadril cair ou subir",
        "Mantenha a respiração regular",
        "Olhe para baixo, mantendo o pescoço neutro"
      ]
    }
  ],
